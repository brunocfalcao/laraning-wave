{
    "name": "brunocfalcao/laraning-wave",
    "description": "Laraning backend video manager",
    "keywords": [
        "brunocfalcao",
        "laraning",
        "wave",
        "waygou"
    ],
    "homepage": "https://github.com/brunocfalcao/laraning-wave",
    "license": "MIT",
    "authors": [
        {
            "name": "Bruno Falcao",
            "email": "bruno.falcao@brunofalcao.me",
            "homepage": "https://www.brunofalcao.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "~5.8.0",
        "spatie/laravel-permission": "^2.0",
        "brunocfalcao/flame":"^1.0",
        "thomaswelton/laravel-gravatar": "^1.1",
        "wildbit/postmark-php": "^2.6",
        "recca0120/terminal": "^1.6",
        "intervention/image": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Laraning\\Wave\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Laraning\\Wave\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laraning\\Wave\\Providers\\LaraningWaveServiceProvider"
            ]
        }
    }
}
